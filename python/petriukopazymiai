paz = int(input('Ka gavo petriukas'))
#begalybes = (x<=0) or (x>=11)

if paz == 10:
    print(f'{paz} - Toks pazimys negalimas')
elif 1<=paz<=3:
    print(f'{paz} - Toks pazimys blogas')
elif 4<=paz<=6:
    print(f'{paz} - Toks pazimys patenkinamas')
elif 7<=paz<=9:
    print(f'{paz} - Toks pazimys geras')
else:
    print(f'{paz} Toks pazimys puikus')